# presumes consul service-init has already been run
sudo cp ./consul/acl.hcl /etc/consul.d/
sudo cp ./consul/connect.hcl /etc/consul.d/
sudo cp ./consul/consul.hcl /etc/consul.d/
sudo cp ./consul/tls-server.json /etc/consul.d/
sudo chmod 640 /etc/consul.d/ -R
sudo chown consul:consul /etc/consul.d -R
